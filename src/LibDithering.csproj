<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>Myitian.Dithering</PackageId>
    <VersionPrefix>0.9.3</VersionPrefix>
    <VersionSuffix>$(VersionSuffix)</VersionSuffix>
    <Authors>Myitian</Authors>
    <Description></Description>
    <IncludeSource>true</IncludeSource>
    <PackageProjectUrl></PackageProjectUrl>
    <RepositoryUrl>https://github.com/Myitian/Dithering</RepositoryUrl>
    <PackageLicenseExpression>Unlicense</PackageLicenseExpression>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <RootNamespace>Myitian.Dithering</RootNamespace>
    <Version>1.0.2</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <AssemblyName>Myitian.Dithering</AssemblyName>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DebugType>none</DebugType>
    <DebugSymbols>false</DebugSymbols>
    <DocumentationFile />
  </PropertyGroup>

</Project>
